extends ../layouts/default

block content
	#airotek_carousel.carousel.slide(data-ride='carousel')
		ol.carousel-indicators(style="background-color:#343434;width:100px;margin:auto;margin-left:-50px;border-radius:15px;")
			li.active(data-target='airotek_carousel', data-slide-to='1')
			li(data-target='airotek_carousel', data-slide-to='2')
			li(data-target='airotek_carousel', data-slide-to='3')
			li(data-target='airotek_carousel', data-slide-to='4')
			li(data-target='airotek_carousel', data-slide-to='5')
			li(data-target='airotek_carousel', data-slide-to='6')
		.carousel-inner(role='listbox')
			.item.active
				img.slide-1(src='/images/slider/1.jpg', alt='First slide')
				.container
					.carousel-caption
						//a.btn.btn-lg.btn-default(href='#', style="border: 1px solid #6EC91E;") Learn More &gt;&gt;
			.item
				img.slide-2(src='/images/slider/2.jpg', alt='Second slide')
				.container
					.carousel-caption
						//a.btn.btn-lg.btn-default(href='#', style="border: 1px solid #6EC91E;") Learn More &gt;&gt;
			.item
				img.slide-3(src='/images/slider/3.jpg', alt='Third slide')
				.container
					.carousel-caption
						//a.btn.btn-lg.btn-default(href='#', style="border: 1px solid #6EC91E;") Learn More &gt;&gt;
			.item
				img.slide-4(src='/images/slider/4.jpg', alt='Fourth slide')
				.container
					.carousel-caption
						//a.btn.btn-lg.btn-default(href='#', style="border: 1px solid #6EC91E;") Learn More &gt;&gt;
			.item
				img.slide-5(src='/images/slider/5.jpg', alt='Fifth slide')
				.container
					.carousel-caption
						//a.btn.btn-lg.btn-default(href='#', style="border: 1px solid #6EC91E;") Learn More &gt;&gt;
			.item
				img.slide-6(src='/images/slider/6.jpg', alt='Sixth slide')
				.container
					.carousel-caption
						//a.btn.btn-lg.btn-default(href='#', style="border: 1px solid #6EC91E;") Learn More &gt;&gt;
		a.left.carousel-control(href='#!/#airotek_carousel', role='button', data-slide='prev')
			span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
			span.sr-only Previous
		a.right.carousel-control(href='#!/#airotek_carousel', role='button', data-slide='next')
			span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
			span.sr-only Next
			//end of carousel

